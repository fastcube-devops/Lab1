        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>210</pd:x>
        <pd:y>182</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>false</validateOutput>
            <term ref="pfx:client"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="tib:render-xml($JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:client)"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>

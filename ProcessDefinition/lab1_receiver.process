                <ns12:logmessage>
                    <xsl:value-of select="concat(&quot;End of process : &quot;,$_processContext/ns3:ProcessContext/ProcessId)"/>
                </ns12:logmessage>
            </ns12:LogInfo>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm">

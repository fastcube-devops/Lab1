    <pd:activity name="Update client">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>749</pd:x>
        <pd:y>93</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/Connection_JDBC.sharedjdbc</jdbcSharedConfig>
            <statement>update clients set date_update = Current_date , flag = 'N' where id_client=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>id_client</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <id_client>
                    <xsl:choose>
                        <xsl:when test="exists($JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:id_client)">
                            <xsl:value-of select="$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:id_client"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </id_client>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>

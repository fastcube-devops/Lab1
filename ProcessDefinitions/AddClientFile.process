                </fileName>
                <textContent>
                    <xsl:value-of select="concat($JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:id_client, ',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:nom, ',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:prenom, ',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:adresse,',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:profession, ',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:date_ajout, ',',$JMS-Queue-Receiver-data-client/ns:ActivityOutput/Body/pfx:infoClient/pfx:date_update)"/>
